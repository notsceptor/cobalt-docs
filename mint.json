{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Cobalt Bot Documentation",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#FFFFFF",
    "light": "#555555",
    "dark": "#888888",
    "background": {
      "light": "#FAFAFA",
      "dark": "#121212"
    },
    "anchors": {
